{
    "env": {
        "browser": true,
        "es6": true,
        "jest": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-restricted-syntax": [
            "error",
            "LabeledStatement",
            "WithStatement"
        ],
        "import/extensions": [
            "error",
            "ignorePackages"
        ],
        "no-console": "off",
        "quotes": "off",
        "func-names": [
            "error",
            "never"
        ],
        "no-use-before-define": [
            "error",
            { "functions": false }
        ],
        "no-unused-vars": "off",
        "no-new": "off",
        "no-else-return": "off",
        "no-plusplus": "off"
    }
}